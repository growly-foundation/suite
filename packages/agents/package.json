{
  "name": "@getgrowly/agents",
  "version": "0.1.0",
  "description": "Modular agent system with multi-agent support for Growly applications",
  "dependencies": {
    "@langchain/anthropic": "^0.3.20",
    "@langchain/aws": "^0.1.9",
    "@langchain/core": "^0.3.51",
    "@langchain/langgraph": "^0.2.67",
    "@langchain/langgraph-checkpoint-postgres": "^0.0.5",
    "@langchain/openai": "^0.5.11",
    "@langchain/tavily": "^0.1.1",
    "@uniswap/sdk-core": "^7.7.2",
    "axios": "^1.9.0",
    "tslog": "^4.9.3",
    "viem": "^2.30.1",
    "zod": "^3.22.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && pnpm bundle:prod",
    "dev:sync": "pnpm run db:sync && pnpm run bundle:dev",
    "dev": "pnpm run bundle:dev",
    "bundle:dev": "NODE_ENV=development vite build --watch",
    "bundle:prod": "vite build",
    "clean": "rm -rf dist esm",
    "format": "prettier --write .",
    "db:sync": "npx supabase gen types typescript --project-id $PROJECT_ID --schema next_auth,public > src/types/database.types.ts",
    "db:reset": "sudo ./src/sql/reset.sh & pnpm run db:sync & pnpm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "glob": "^11.0.2",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.10",
    "rollup-plugin-preserve-use-client": "^3.0.1",
    "ts-jest": "^29.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-externalize-deps": "^0.9.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@10.6.3",
  "peerDependencies": {
    "@getgrowly/core": "workspace:*"
  }
}
